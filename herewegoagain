create database autoker;


use database autoker;
CREATE TABLE `autok` (
  `rendsz` char(7) COLLATE utf8_hungarian_ci NOT NULL,
  `marka` varchar(30) COLLATE utf8_hungarian_ci NOT NULL,
  `ev` int(11) NOT NULL,
  `motor` int(11) NOT NULL,
  `ar` int(11) NOT NULL,
  `ajtok` int(11) NOT NULL,
  `seb` int(11) NOT NULL,
  `loero` int(11) NOT NULL,
  `fogy` decimal(4,2) NOT NULL,
  `szin` varchar(30) COLLATE utf8_hungarian_ci NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_hungarian_ci;



